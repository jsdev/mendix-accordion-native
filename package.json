{
  "name": "faqaccordion",
  "widgetName": "FAQAccordion",
  "version": "1.0.0",
  "description": "FAQ Accordion widget for Mendix with rich text support and show/hide all functionality",
  "license": "Apache-2.0",
  "author": "Anthony Delorie",
  "config": {
    "projectPath": "../MendixProject/",
    "mendixHost": "http://localhost:8080",
    "developmentPort": 3000
  },
  "packagePath": "mendix",
  "scripts": {
    "start": "pluggable-widgets-tools start:server",
    "dev": "pluggable-widgets-tools start:web",
    "build": "pluggable-widgets-tools build:web",
    "release": "pluggable-widgets-tools release:web && node scripts/strip-console-logs.js",
    "format": "pluggable-widgets-tools format",
    "lint": "pluggable-widgets-tools lint",
    "test": "jest --config jest.config.js --coverage --watchAll=false",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@mendix/pluggable-widgets-tools": "^10.24.0",
    "@types/archiver": "^7.0.0",
    "@types/jest": "^29.5.12",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "babel-jest": "^30.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "terser": "^5.44.1",
    "ts-jest": "^29.1.2"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/dompurify": "^3.0.5",
    "classnames": "^2.5.1",
    "dompurify": "^3.3.1",
    "marked": "^17.0.1"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "@types/react": "18.2.0",
      "@types/react-dom": "18.2.0"
    }
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
